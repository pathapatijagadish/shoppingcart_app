<h1>Categories List</h1>
<table>
	<tr>
		<td>Category Name</td>
	</tr>
	<% @categories.each do |c| %>
	<tr>
		<td><%= c.name%></td>
		<td><%= link_to "Edit",edit_category_path(c)%></td>
		<td><%= link_to "delete",category_path(c),:method=>:delete%></td>
	</tr>
	<%end%>
</table>
</br>
<%= link_to "Add Category",new_category_path%>


